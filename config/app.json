{

    "view engines": {
        "js": {
            "module": "adaro",
            "factoryMethod": "js",
            "arguments": [{
                "helpers": null,
                "stream": false
            }]
        },

        "dust": {
            "module": "adaro",
            "factoryMethod": "dust",
            "arguments": [{
                "helpers": null,
                "stream": false
            }]
        },

        "html": {
            "module": "consolidate",
            "name": "swig"
        }
    },

    "express": {
        "env": "", // NOTE: `env` is managed by the framework. This value will be overwritten.
        "x-powered-by": false,
        "trust proxy": false,
        "jsonp callback name": null,
        "json replacer": null,
        "json spaces": 0,
        "case sensitive routing": false,
        "strict routing": false,
        "view cache": true,
        "view engine": "js",
        "views": "path:./.build/templates",
        "route": "/"
    }
}