{
    "middleware": {

        "shutdown": {
            "enabled": true,
            "priority": 0,
            "module": "kraken-js/lib/middleware/shutdown",
            "arguments": [{
                "timeout": 30000,
                "template": null
            }]
        },

        "favicon": {
            "enabled": true,
            "priority": 10,
            "module": "express"
        },

        "static": {
            "enabled": true,
            "priority": 20,
            "module": "express",
            "arguments": [ "path:./.build" ]
        },

        "logger": {
            "enabled": true,
            "priority": 30,
            "module": "express"
        },

        "json": {
            "enabled": true,
            "priority": 40,
            "module": "express"
        },

        "urlencoded": {
            "enabled": true,
            "priority": 50,
            "module": "express"
        },

        "cookieParser": {
            "enabled": true,
            "priority": 60,
            "module": "express",
            "arguments": [ "keyboard cat" ]
        },

        "session": {
            "enabled": true,
            "priority": 70,
            "module": "express",
            "arguments": [
                {
                    "key": "",
                    "secret": "keyboard cat",
                    "cookie": {
                        "path": "/",
                        "httpOnly": true,
                        "maxAge": null
                    },
                    "proxy": null
                }
            ]
        },

        "appsec": {
            "enabled": true,
            "priority": 80,
            "module": "lusca",
            "arguments": [
                {
                    "csrf": true,
                    "xframe": "SAMEORIGIN",
                    "p3p": "NOI DSP CUR ADMa OUR IND NAV STA PUR",
                    "csp": false
                }
            ]
        },

        "router": {
            "enabled": true,
            "priority": 90,
            "module": "express-enrouten",
            "arguments": [ { "directory": "path:./controllers" } ]
        }

    }

}